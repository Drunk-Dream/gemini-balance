# Google Gemini API 密钥，多个密钥用逗号分隔。
# 例如：GOOGLE_API_KEYS="your_api_key_1,your_api_key_2"
GOOGLE_API_KEYS=

# Google Gemini API 的基础 URL。
# 默认值：https://generativelanguage.googleapis.com
GEMINI_API_BASE_URL=https://generativelanguage.googleapis.com

# OpenAI API 的基础 URL。
# 默认值：https://generativelanguage.googleapis.com (注意：这里可能需要根据实际OpenAI API地址调整)
OPENAI_API_BASE_URL=https://generativelanguage.googleapis.com

# 日志级别 (DEBUG, INFO, WARNING, ERROR, CRITICAL)。
# 默认值：INFO
LOG_LEVEL=INFO

# 是否启用调试日志。设置为 "true" 启用，"false" 禁用。
# 默认值：False
DEBUG_LOG_ENABLED=False

# 调试日志文件的路径。
# 默认值：logs/gemini_debug.log
DEBUG_LOG_FILE=logs/gemini_debug.log

# API 密钥冷却时间（秒），当密钥失败时，该密钥将在此时间内不被使用。
# 默认值：300 (5分钟)
API_KEY_COOL_DOWN_SECONDS=300

# API 密钥失败阈值，当密钥失败次数达到此值时，将进入冷却期。
# 默认值：3
API_KEY_FAILURE_THRESHOLD=3

# API 密钥最大冷却时间（秒），防止无限期冷却。
# 默认值：43200 (12小时)
MAX_COOL_DOWN_SECONDS=43200
